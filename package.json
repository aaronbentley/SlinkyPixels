{
  "name": "slinky-pixels",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https",
    "dev:turbo": "next dev --experimental-https --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "preview": "next build && next start",
    "preview:turbo": "yarn build:turbo && yarn start",
    "lint": "next lint",
    "schema:extract": "sanity schema extract --enforce-required-fields --path='./src/sanity/extract.json'",
    "schema:generate": "sanity typegen generate",
    "typegen": "yarn schema:extract && yarn schema:generate"
  },
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "1.2.0",
    "@sanity/types": "^4.8.1",
    "@sanity/ui": "^3.1.0",
    "@sanity/vision": "4.8.1",
    "geist": "^1.5.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-sanity": "^11.1.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sanity": "4.8.1",
    "shadcn": "^3.3.0",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-is": "^19.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-is": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.9.2"
}
